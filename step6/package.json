{
  "name": "agent-orchestration-lab",
  "version": "0.1.0",
  "private": true,
  "description": "Learning-focused multi-agent orchestration lab with dev/test agents and a supervisor",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "cli": "tsx src/cli.ts",
    "lint": "node -e \"console.log('lint ok')\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "study:1": "tsx src/study/step1-dev-agent.ts",
    "study:2": "tsx src/study/step2-dev-test-agent.ts",
    "study:3": "tsx src/study/step3-supervisor-loop.ts",
    "study:4": "tsx src/study/step4-supervisor-with-services.ts"
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "diff": "^7.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "openai": "^4.104.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/diff": "^7.0.1",
    "@types/node": "^22.9.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
